    	</LIGHT>
	</LIGHTS>

	<TEXTURES>
    	<TEXTURE id="wall-tex">
        	<file path="images/wall.png" />            <!-- path to file -->
        	<amplif_factor s="3" t="1" />                <!-- x/s, y/t -->
    	</TEXTURE>
    	<TEXTURE id="floor-tex">
        	<file path="images/floor.png" />           <!-- path to file -->
        	<amplif_factor s="5" t="6" />                <!-- x/s, y/t -->
    	</TEXTURE>
    	<TEXTURE id="darkwood-tex">
