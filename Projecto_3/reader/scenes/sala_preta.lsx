        	<amplif_factor s="1" t="1" />                <!-- x/s, y/t -->
    	</TEXTURE>
    	<TEXTURE id="metal-tex">
        	<file path="images/metal2.png" />           <!-- path to file -->
        	<amplif_factor s="1" t="1" />                <!-- x/s, y/t -->
    	</TEXTURE>
    	<TEXTURE id="lamp-tex">
        	<file path="images/lamp2.png" />            <!-- path to file -->
