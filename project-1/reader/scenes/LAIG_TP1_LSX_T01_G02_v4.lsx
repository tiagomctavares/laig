            <DESCENDANT id="tri_telhado" />
        </DESCENDANTS>
    </NODE>

    <NODE id="tel3">
        <MATERIAL id="null" />     
		
        <TEXTURE id="null" />

        <TRANSLATION x="2" y="0" z="2" />
        <ROTATION axis="y" angle="180" />
        <ROTATION axis="z" angle="-30" />

        <DESCENDANTS>
            <DESCENDANT id="tri_telhado" />
        </DESCENDANTS>
    </NODE>

    <NODE id="tel4">
        <MATERIAL id="null" /> 
		
        <TEXTURE id="null" />

        <TRANSLATION x="2" y="0" z="0" />
        <ROTATION axis="y" angle="-90" />
        <ROTATION axis="z" angle="-30" />

        <DESCENDANTS>
            <DESCENDANT id="tri_telhado" />
        </DESCENDANTS>
    </NODE>

    <NODE id="paredes">
        <MATERIAL id="base" /> 
		
        <TEXTURE id="text_parede" />

        <TRANSLATION x="0" y="-2" z="0" />

        <DESCENDANTS>
            <DESCENDANT id="par1" />
            <DESCENDANT id="par2" />
            <DESCENDANT id="par3" />
            <DESCENDANT id="par4" />
            <DESCENDANT id="par5" />
        </DESCENDANTS>
    </NODE>

    <NODE id="par1">
        <MATERIAL id="null" />  
		
        <TEXTURE id="text_porta" />

        <TRANSLATION x="0" y="0" z="2" />

        <DESCENDANTS>
            <DESCENDANT id="rec_par" />
        </DESCENDANTS>
    </NODE>

    <NODE id="par2">
        <MATERIAL id="null" />      

        <TEXTURE id="null" />

        <TRANSLATION x="2" y="0" z="2" />
        <ROTATION axis="y" angle="90" />

        <DESCENDANTS>
            <DESCENDANT id="rec_par" />
        </DESCENDANTS>
    </NODE>

    <NODE id="par3">
        <MATERIAL id="null" />      

        <TEXTURE id="null" />

        <TRANSLATION x="2" y="0" z="0" />
        <ROTATION axis="y" angle="180" />

        <DESCENDANTS>
            <DESCENDANT id="rec_par" />
        </DESCENDANTS>
    </NODE>

    <NODE id="par4">
        <MATERIAL id="null" />      

        <TEXTURE id="null" />

        <TRANSLATION x="0" y="0" z="0" />
        <ROTATION axis="y" angle="-90" />

        <DESCENDANTS>
            <DESCENDANT id="rec_par" />
        </DESCENDANTS>
    </NODE>

    <NODE id="par5">
        <MATERIAL id="null" />      

        <TEXTURE id="null" />

        <TRANSLATION x="0" y="0" z="0" />
        <ROTATION axis="x" angle="90" />

