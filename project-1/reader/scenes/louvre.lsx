			</DESCENDANTS>
        </NODE>
        
        
         <NODE id="calcada">       
			<MATERIAL id="null" /> 
            <TEXTURE id="rua" />  
             <TRANSLATION x="-5" y="-0.05" z="8.5"/>
              <SCALE sx="8" sy="0" sz="8"/>
              <ROTATION axis="x" angle="-90"/>
			<DESCENDANTS>
                <DESCENDANT id="quadrado"/>
			</DESCENDANTS>
        </NODE>
        
        <NODE id="piramide">       
			<MATERIAL id="piramide" /> 
            <TEXTURE id="exterior" />  
			<DESCENDANTS>
                <DESCENDANT id="1trianguloContrario"/>
                <DESCENDANT id="2trianguloContrario"/>
                <DESCENDANT id="3trianguloContrario"/>
                <DESCENDANT id="1triangulo"/>
