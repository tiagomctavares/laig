			<file path="images/palacioContrario.jpg"/>                            
			<amplif_factor s="3.6" t="1" />               
         </TEXTURE>
           <TEXTURE id="padrao">
			<file path="images/padrao.jpg"/>                            
			<amplif_factor s="1" t="1" />               
		</TEXTURE>
         
          <TEXTURE id="parede">
			<file path="images/parede.jpg"/>                            
			<amplif_factor s="4" t="2" />               
		</TEXTURE>
         
         <TEXTURE id="texturaEsfera">
			<file path="images/texturaEsfera.jpg"/>                            
			<amplif_factor s="1" t="1" />               
		</TEXTURE>
	 </TEXTURES>

	 <MATERIALS>
	
     
         <MATERIAL id="parede">                                  
			    <shininess value="0" />
                <specular r="0" g="0" b="0" a="1" />        <!-- specular reflection -->
                <diffuse r="0.2" g="0.17" b="0.07" a="1" />   <!-- diffuse reflection -->
                <ambient r="0.2" g="0.17" b="0.07" a="1" />         <!-- ambient reflection -->
                <emission r="0" g="0" b="0" a="1" />        <!-- emission component -->   
		</MATERIAL>
        
         
        <MATERIAL id="default">                                  
           <shininess value="120" />
            <specular r="0" g="0" b="0" a="1" />        <!-- specular reflection -->
            <diffuse r="0.5" g="0.5" b="0.5" a="1" />   <!-- diffuse reflection -->
            <ambient r="0.9" g="0.9" b="0.9" a="1" />         <!-- ambient reflection -->
            <emission r="0" g="0" b="0" a="0" />           <!-- emission component -->
        </MATERIAL>
         
         <MATERIAL id="piramide">                                  
            <shininess value="0" />
            <specular r="0.2" g="0.2" b="0.2" a="1" />        <!-- specular reflection -->
            <diffuse r="0.4" g="0.4" b="0.4" a="1" />   <!-- diffuse reflection -->
            <ambient r="0.7" g="0.7" b="0.7" a="1" />         <!-- ambient reflection -->
            <emission r="0" g="0" b="0" a="0" />          <!-- emission component -->
        </MATERIAL>
         
         
         
         <MATERIAL id="palacio">                                  
			<shininess value="120" />
			<specular r="0" g="0" b="0" a="1" />        
			<diffuse r="0" g="0" b="0" a="1" />         
			<ambient r="1" g="1" b="1" a="1" />        
			<emission r="0" g="0" b="0" a="1" />       
            </MATERIAL>
    </MATERIALS>

	<LEAVES>
		<LEAF id="retangulo" type="rectangle" args="0.6 1 3.4 0" />
        <LEAF id="moldura" type="rectangle" args="0 0.7 0.7 0" />
        <LEAF id="esfera" type="sphere" args="0.1 30 30" />
        <LEAF id="triangulo" type="triangle" args="0 0 0 4 0 0 2 4 0" />
        <LEAF id="cilindro" type="cylinder" args="0.9 0.1 0.1 1 40"/>
        <LEAF id="quadrado" type="rectangle" args="0 2.8 2.8 0" />
        
	</LEAVES>
	

	<NODES>
		<ROOT id="louvre" />    
        
		<NODE id="louvre">       
			<MATERIAL id="default" /> 
            <TEXTURE id="null" />  
            <SCALE sx="2" sy="2" sz="2"/>
			<DESCENDANTS>
                <DESCENDANT id="exterior"/>
                <DESCENDANT id="edificios"/>
                <DESCENDANT id="interior"/>
			</DESCENDANTS>
		</NODE>
              
          <NODE id="exterior">       
			<MATERIAL id="piramide" /> 
            <TEXTURE id="null" />
			<DESCENDANTS>
                <DESCENDANT id="piramide"/>
			</DESCENDANTS>
        </NODE>
        
        <NODE id="esferas">       
			<MATERIAL id="piramide" /> 
            <TEXTURE id="texturaEsfera" />
			<DESCENDANTS>
                <DESCENDANT id="esfera1"/>
                <DESCENDANT id="esfera2"/>
                <DESCENDANT id="esfera3"/>
                <DESCENDANT id="esfera4"/>
			</DESCENDANTS>
        </NODE>
        
        <NODE id="esfera1">       
			<MATERIAL id="null" /> 
            <TEXTURE id="null" />
            <TRANSLATION x="-0.5" y="0.4" z="-0.5"/>
            <SCALE sx="4" sy="4" sz="4"/>
			<DESCENDANTS>
