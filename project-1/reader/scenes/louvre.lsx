<?xml version="1.0" encoding="UTF-16" standalone="yes"?>
<SCENE>
	  <INITIALS>
        <frustum near="0.1" far="500.0"/>     
        <translation x="-7" y="-7" z="-4" />    
        <rotation axis="z" angle="20" />           
        <rotation axis="y" angle="2" />           
        <rotation axis="x" angle="-20" />          
        <scale sx="1.0" sy="1.0" sz="1.0" />      
        <reference length="0" />                 
    </INITIALS>
    
	<ILLUMINATION>
		<ambient r="0" g="0" b="0" a="1" />            
		<doubleside value="0" />                         
		<background r="0" g="0" b="0.1" a="1" />        
	</ILLUMINATION>

	<LIGHTS>
		
    
        <LIGHT id="primeira">                                  
			<enable value ="1" />                       
			<position x="4" y="2" z="6" w="1" />      
			<ambient r="0" g="0" b="0" a="1" />        <!-- ambient component -->
             <diffuse r="1" g="1" b="1" a="1" />          <!-- diffuse component -->
             <specular r="0" g="0" b="0" a="1" />      
		</LIGHT>
        
        <LIGHT id="segunda">                                  
			<enable value ="1" />                       
			<position x="4" y="2" z="2" w="1" />      
			<ambient r="0" g="0" b="0" a="1" />        <!-- ambient component -->
            <diffuse r="1" g="1" b="1" a="1" />        <!-- diffuse component -->
            <specular r="0" g="0" b="0" a="1" />   
		</LIGHT>
        
        <LIGHT id="terceira">                                  
			<enable value ="1" />                       
			<position x="2" y="2" z="4" w="1" />      
			<ambient r="0" g="0" b="0" a="1" />        <!-- ambient component -->
             <diffuse r="1" g="1" b="1" a="1" />        <!-- diffuse component -->
             <specular r="0" g="0" b="0" a="1" />     
		</LIGHT>
        
        <LIGHT id="quinta">                                  
			<enable value ="1" />                       
			<position x="4" y="10" z="4" w="1" />      
			<ambient r="0" g="0" b="0" a="1" />        <!-- ambient component -->
             <diffuse r="1" g="1" b="1" a="1" />          <!-- diffuse component -->
             <specular r="1" g="1" b="1" a="1" />      
		</LIGHT>
        
      
        <LIGHT id="Sol">                                  
			<enable value ="1" />                       
			<position x="5" y="20" z="5" w="1" />      
			<ambient r="0.8" g="0.8" b="0.8" a="1" />         <!-- ambient component -->
            <diffuse r="0" g="0" b="0" a="1" />        <!-- diffuse component -->
            <specular r="0" g="0" b="0" a="1" />  
		</LIGHT>
   
     </LIGHTS>

     <TEXTURES>   
        <TEXTURE id="exterior">
			<file path="images/louvre.jpg"/>                            
			<amplif_factor s="3.9" t="3.9" />               
		</TEXTURE>  
         <TEXTURE id="vangogh">
			<file path="images/vangogh.jpg"/>                            
			<amplif_factor s="1" t="1" />               
		</TEXTURE>  
          <TEXTURE id="monalisa">
			<file path="images/monalisa.jpg"/>                            
			<amplif_factor s="0.7" t="0.7" />               
		</TEXTURE>
         <TEXTURE id="dali">
			<file path="images/dali.jpg"/>                            
			<amplif_factor s="0.7" t="0.7" />               
		</TEXTURE>
         <TEXTURE id="rua">
			<file path="images/calcada.jpg"/>                            
			<amplif_factor s="0.1" t="0.1" />               
		</TEXTURE>
          <TEXTURE id="chaoInterior">
			<file path="images/pavimento.jpg"/>                            
			<amplif_factor s="0.2" t="0.2" />               
		</TEXTURE>
         <TEXTURE id="palacio">
			<file path="images/palacio.jpg"/>                            
			<amplif_factor s="3.4" t="1" />               
		</TEXTURE>
